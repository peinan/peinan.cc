{
  "name": "peinan-cc",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "prestart": "yarn run build:tailwind",
    "start": "next start",
    "prebuild": "yarn run build:tailwind",
    "build:tailwind": "tailwindcss build src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "build": "next build && node .next/server/build-rss.js",
    "postbuild": "next-sitemap",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "lint-staged": "lint-staged"
  },
  "pre-commit": "lint-staged",
  "dependencies": {
    "@types/gtag.js": "^0.0.8",
    "@zeit/react-jsx-parser": "2.0.0",
    "async-sema": "3.1.0",
    "autoprefixer": "^10.1.0",
    "github-slugger": "1.2.1",
    "katex": "0.12.0",
    "line-awesome": "^1.3.0",
    "masonic": "^3.4.0",
    "next": "^11.1.2",
    "next-sitemap": "^1.6.57",
    "postcss": "^8.2.1",
    "prismjs": "1.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-loading-skeleton": "^2.2.0",
    "react-use-scrollspy": "^3.0.0",
    "tailwindcss": "^2.0.2",
    "uuid": "8.1.0"
  },
  "devDependencies": {
    "@types/katex": "0.11.0",
    "@types/node": "14.14.31",
    "@types/react": "17.0.2",
    "lint-staged": "10.5.4",
    "pre-commit": "1.2.2",
    "prettier": "2.2.1",
    "typescript": "^4.4.4"
  }
}
